{
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\Engine\\": "src/engine/",
      "App\\Mutator\\": "src/custom-mutators/"
    }
  },
  "require": {
    "infection/mutator": "^0.4.0",
    "nikic/php-parser": "^5.4",
    "elephantio/elephant.io": "^4.13",
    "google-gemini-php/client": "^2.1",
    "symfony/http-client": "^7.2",
    "nyholm/psr7": "^1.8",
    "psr/http-client": "^1.0",
    "mozex/anthropic-php": "^1.1",
    "openai-php/client": "^0.13.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5",
    "infection/infection": "^0.29.10"
  },
  "config": {
      "allow-plugins": {
      "infection/extension-installer": true,
      "php-http/discovery": true
    }
  }
}